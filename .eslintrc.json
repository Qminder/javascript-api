{
  "extends": [
    "airbnb-base",
    "airbnb-flow"
  ],
  "env": {
    "jquery": true,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "no-param-reassign": [
      "error", {
        "props": true,
        "ignorePropertyModificationsFor": [
          "$scope",
          "state"
        ]
      }
    ],
    "no-console": [
      "error", {
        "allow": [
          "warn",
          "error",
          "log"
        ]
      }
    ]
  }
}
