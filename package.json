  "types": "build/qminder-api.d.ts",
  "repository": "Qminder/javascript-api",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/sinon": "^9.0.10",
    "jest": "^26.6.3",
